{
  "name": "davinci-design",
  "homepage": "https://kyeb.github.io/davinci-design/",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "rm -rf .parcel-cache dist && parcel src/index.html",
    "typecheck": "tsc --noEmit",
    "build": "rm -rf .parcel-cache dist && parcel build src/index.html --public-url /davinci-design/ && cp src/404.html dist/",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.12.0",
    "@types/react": "^18.2.78",
    "@types/three": "^0.163.0",
    "gh-pages": "^6.1.1",
    "parcel": "^2.12.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "dependencies": {
    "@react-three/drei": "^9.105.4",
    "@react-three/fiber": "^8.16.2",
    "fomantic-ui": "^2.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "three": "^0.163.0",
    "three-stdlib": "^2.29.6"
  },
  "license": "MIT"
}
